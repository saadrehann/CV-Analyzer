{
    "builds": [
        {
            "src": "cv_rater_backend/wsgi.py",
            "use": "@vercel/python""config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        ],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "cv_rater_backend/wsgi.py"
            }
        ]
    }